{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@layouts/*": ["src/layouts/*"],
      "@sections/*": ["src/sections/*"],
      "@lib/*": ["src/lib/*"],
      "@icons/*": ["src/icons/*"],
      "@i18n/*": ["src/i18n/*"],
      "@data/*": ["src/data/*"]
    }
  }
}
